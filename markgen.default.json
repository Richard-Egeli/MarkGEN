{
    "project": "Example Documentation",
    "outDir": "build",
    "srcDir": "src",
    "baseDir": "",
    "assetDir": "assets",
    "codeTheme": "atom-one-dark",
    "extensions": [
        "md",
        "ts"
    ],
    "compilationOptions": {
        "compileScripts": true,
        "compileCss": true,
        "removeEmptyDirs": true
    },
    "colorPalette": {
        "light": {
            "primary": "#EAEEED",
            "secondary": "#2980b9",
            "background": "#EEE",
            "border": "#DDD",
            "text": "#4C4957",
            "title": "#fff",
            "textSecondary": "#EEE",
            "buttonPrimary": "#EAEEED",
            "buttonSecondary": "#DADADA"
        },
        "dark": {
            "primary": "#EEE",
            "secondary": "#2a2a2a",
            "background": "#2a2a2a",
            "border": "#DDD",
            "text": "#EEE",
            "textSecondary": "#2a2a2a"
        }
    }
}
